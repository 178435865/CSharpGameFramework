skilltemplate("普通攻击")
{:
  section(1000)
  {
    animation(Attack);
    //adjustsectionduration("anim",100);
    selfeffect(selfEffect,1000,"hit",0);
    impact(0,0,0,0,false);
  };  
  hitskill
  { 
  	section(1000)
  	{
	    animation(Hit);
	    selfeffect(hitEffect, 1000, hit, 0);
	    damage(0);   
	    lockframe(0,0,166);
	  };
  };  
:};
