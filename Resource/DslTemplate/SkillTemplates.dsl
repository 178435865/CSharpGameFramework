skilltemplate("控制buff")
{:
  section(10)
  {
    enablemoveagent(false);
    addstate("silence");
    storepos();
  };
  section(300)
  {
    animation(Fukong);
    adjustsectionduration('anim',200);
  };
  onstop
  {
    restorepos();
    enablemoveagent(true);
    removestate("silence");
  };
:};